{
  "name": "orcnd/tcgame",
  "description": "Tcgame",
  "licence": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "App\\Init::install"
    ],
    "test": [
      "phpunit tests/*.php"
    ]
  }
}
